	<queryString>
		<![CDATA[SELECT cp.TaxID, cp.Name, fc.DocumentNo Credit, cd.DocumentNo CreditDefiniton, lot.Name PlantingCycle, ft.DateDoc, SUM(il.QtyInvoiced) QtyInvoiced,
pr.Name ||  '(' || ROUND(pa.ApprovedQty*fc.ApprovedQty, 2) || ' ' || uom.UomSymbol || ')' Description
FROM FTA_CreditDefinition cd
INNER JOIN M_Lot lot ON(lot.M_Lot_ID = cd.PlantingCycle_ID)
INNER JOIN FTA_ProductListApproved pa ON(pa.FTA_CreditDefinition_ID = cd.FTA_CreditDefinition_ID)
INNER JOIN M_Product pr ON(pa.M_Product_ID = pr.M_Product_ID)
INNER JOIN FTA_RV_DocumentFact ft ON(ft.FTA_CreditDefinition_ID = cd.FTA_CreditDefinition_ID)
INNER JOIN C_InvoiceLine il ON(il.C_Invoice_ID = ft.Record_ID AND (il.M_Product_ID = pa.M_Product_ID OR il.M_Product_ID = pa.Substitute_ID))
INNER JOIN C_UOM uom ON(uom.C_UOM_ID = pr.C_UOM_ID)
INNER JOIN C_BPartner cp ON(cp.C_BPartner_ID = ft.C_BPartner_ID)
INNER JOIN FTA_FarmerCredit fc ON(fc.FTA_FarmerCredit_ID = ft.FTA_FarmerCredit_ID)
WHERE (ft.C_BPartner_ID=$P{C_BPartner_ID} OR $P{C_BPartner_ID} IS NULL)
AND (ft.FTA_CreditDefinition_ID=$P{FTA_CreditDefinition_ID} OR $P{FTA_CreditDefinition_ID} IS NULL)
AND (ft.FTA_FarmerCredit_ID=$P{FTA_FarmerCredit_ID} OR $P{FTA_FarmerCredit_ID} IS NULL)
AND (ft.DateDoc >= $P{DateDoc1} /*OR  IS NULL*/)
AND (ft.DateDoc <= $P{DateDoc2} /*OR  IS NULL*/)
GROUP BY cp.TaxID, cp.Name, fc.DocumentNo, cd.DocumentNo, lot.Name, ft.DateDoc, il.M_Product_ID, il.C_Charge_ID, il.Description, pr.Name, pa.ApprovedQty, uom.UomSymbol, fc.ApprovedQty
HAVING(SUM(il.QtyInvoiced) > 0)
ORDER BY ft.DateDoc]]>
	</queryString>
