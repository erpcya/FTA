	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="FTA_CreditAct_ID" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="FTA_FarmerCredit_ID" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
	fc.AD_Org_ID,
	fc.AD_Client_ID,
	fc.CreatedBy,
	fc.Created,
	fc.Updated,
	fc.UpdatedBy,
	fc.IsActive,
	oi.TaxID,
	COALESCE(bp.TaxID,bp.Value) BPTaxID,
	bp.Name||' '||COALESCE(bp.Name2,'') BPName,
	COALESCE(f.Name,MAX(df.Name), '') FarmName,
	ROUND(COALESCE(f.Area, MAX(df.Area)),2) Area,
	COALESCE(f.FrontierNorth,MAX(df.FrontierNorth),'') FrontierNorth,
	COALESCE(f.FrontierSouth, MAX(df.FrontierSouth),'') FrontierSouth,
	COALESCE(f.FrontierEast, MAX(df.FrontierEast), '') FrontierEast,
	COALESCE(f.FrontierWest, MAX(df.FrontierWest),'') FrontierWest,
	COALESCE(p.Name, '') as Name,
	ROUND(fc.ApprovedAmt,2) ApprovedAmt,
	ROUND(fc.ApprovedQty,2) ApprovedQty,
	l.Name PlantingCycle_ID,
	ROUND(fc.Amt,2) Amt,
	c.Name ClientName,
	bp.taxid ,
	bp.nationality,
	COALESCE(MAX(adP.Address1),'') ||' '|| COALESCE(MAX(adP.Address2),'') ||' '|| COALESCE(MAX(adP.Address3), '') ||' '|| COALESCE(MAX(adP.Address4),'') address,
	ROUND(cd.amt,2) DefinedAmt,
	COALESCE(lo.Address1, MAX(df.Address1),'') ||' '|| COALESCE(lo.Address2, MAX(df.Address2),'') ||' '|| COALESCE(lo.Address3, MAX(df.Address3), '') ||' '|| COALESCE(lo.Address4, MAX(df.Address4),'') C_BPartner_Location_ID,
dta.DocumentNote
FROM FTA_FarmerCredit fc
INNER JOIN C_BPartner bp ON (bp.C_BPartner_ID = fc.C_BPartner_ID)
INNER JOIN AD_Client c ON (fc.AD_Client_ID = c.AD_Client_ID)
INNER JOIN FTA_CreditDefinition cd ON (cd.FTA_CreditDefinition_ID = fc.FTA_CreditDefinition_ID)
INNER JOIN FTA_CreditAct act ON(act.FTA_CreditAct_ID = fc.FTA_CreditAct_ID)
INNER JOIN C_DocType dta ON(dta.C_DocType_ID = act.C_DocType_ID)
LEFT JOIN FTA_Farming fa ON (fa.FTA_FarmerCredit_ID = fc.FTA_FarmerCredit_ID)
LEFT JOIN FTA_FarmDivision fd ON (fd.FTA_FarmDivision_ID = fa.FTA_FarmDivision_ID)
LEFT JOIN FTA_Farm f ON (f.FTA_Farm_ID = fd.FTA_Farm_ID)
LEFT JOIN M_Product p ON (p.M_Product_ID = fa.Category_ID)
LEFT JOIN M_Lot l ON (l.M_Lot_ID = cd.PlantingCycle_ID)
LEFT JOIN C_BPartner_Location bpl ON (bpl.C_BPartner_Location_ID = f.C_BPartner_Location_ID)
LEFT JOIN (SELECT lo.Address1, lo.Address2, lo.Address3, lo.Address4, loc.C_BPartner_ID FROM C_BPartner_Location loc INNER JOIN C_Location lo ON (loc.C_location_ID = lo.C_Location_ID) WHERE loc.IsBillTo='Y') adP ON ( adP.C_BPartner_ID =fc.C_BPartner_ID)

LEFT JOIN C_Location lo ON (lo.C_Location_ID = bpl.C_Location_ID)
LEFT JOIN AD_OrgInfo oi ON (oi.AD_Org_ID = fc.AD_Org_ID)
LEFT JOIN (SELECT df.C_BPartner_ID, df.FrontierNorth, df.FrontierSouth, df.FrontierEast, df.FrontierWest,
		lo.Address1, lo.Address2, lo.Address3, lo.Address4, df.Name, df.Area
		FROM FTA_Farm df
		INNER JOIN C_BPartner_Location bpl ON (bpl.C_BPartner_Location_ID = df.C_BPartner_Location_ID)
		INNER JOIN C_Location lo ON (lo.C_Location_ID = bpl.C_Location_ID)
		WHERE df.IsActive = 'Y'
		ORDER BY df.IsDefault DESC
	) df ON(df.C_BPartner_ID = fc.C_BPartner_ID)
WHERE  (fc.FTA_CreditAct_ID = $P{FTA_CreditAct_ID} OR $P{FTA_CreditAct_ID} IS NULL)
AND (fc.FTA_FarmerCredit_ID = $P{FTA_FarmerCredit_ID} OR $P{FTA_FarmerCredit_ID} IS NULL)

GROUP BY fc.AD_Org_ID, fc.AD_Client_ID, fc.CreatedBy, fc.Created, fc.Updated, fc.UpdatedBy, fc.IsActive, oi.TaxID, bp.TaxID, bp.Value, bp.Name, bp.Name2, f.Name, f.Area, f.FrontierNorth, f.FrontierSouth, f.FrontierEast, f.FrontierWest,
p.Name, fc.ApprovedAmt, fc.ApprovedQty, l.Name, fc.Amt, c.Name, lo.Address1, lo.Address2, lo.Address3, lo.Address4,
DefinedAmt, nationality, dta.DocumentNote

ORDER BY BPName
LIMIT 1]]>
	</queryString>
