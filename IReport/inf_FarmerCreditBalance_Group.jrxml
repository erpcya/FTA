	<queryString>
		<![CDATA[SELECT cl.Name Client, cp.TaxID, cp.Name, fc.DocumentNo Credit, cd.DocumentNo CreditDefiniton, lot.Name PlantingCycle, MAX(fc.Qty) Qty, MAX(fc.ApprovedQty) ApprovedQty, SUM(fm.EffectiveArea) EffectiveArea, cb.LineDescription, rl.PrintName CDLCategory, cb.SO_CreditLimit, cb.SO_CreditUsed, cb.Balance, rg.FTA_CDL_ReportGroup_ID, rg.PrintName ReportGroup
FROM FTA_CDL_Report r
INNER JOIN FTA_CDL_ReportLine rl ON(rl.FTA_CDL_Report_ID = r.FTA_CDL_Report_ID)
INNER JOIN FTA_RV_CreditDefinitionBalance cb ON(cb.FTA_CDL_Category_ID = rl.FTA_CDL_Category_ID)
INNER JOIN FTA_CreditDefinition cd ON(cd.FTA_CreditDefinition_ID = cb.FTA_CreditDefinition_ID)
INNER JOIN C_BPartner cp ON(cp.C_BPartner_ID = cb.C_BPartner_ID)
INNER JOIN AD_Client cl ON(cl.AD_Client_ID = cd.AD_Client_ID)
INNER JOIN M_Lot lot ON(lot.M_Lot_ID = cd.PlantingCycle_ID)
INNER JOIN FTA_FarmerCredit fc ON(fc.FTA_FarmerCredit_ID = cb.FTA_FarmerCredit_ID)
INNER JOIN FTA_Farming fm ON(fm.FTA_FarmerCredit_ID = fc.FTA_FarmerCredit_ID)
LEFT JOIN FTA_CDL_ReportGroup rg ON(rg.FTA_CDL_ReportGroup_ID = rl.FTA_CDL_ReportGroup_ID)
WHERE r.FTA_CDL_Report_ID=$P{FTA_CDL_Report_ID}
AND ((cb.AD_Table_ID <> 259 AND cb.AD_Table_ID <> 53566 AND cb.AD_Table_ID <> 335) OR cb.AD_Table_ID IS NULL)
AND (cb.C_BPartner_ID=$P{C_BPartner_ID} OR $P{C_BPartner_ID} IS NULL)
AND (cb.FTA_CreditDefinition_ID=$P{FTA_CreditDefinition_ID} OR $P{FTA_CreditDefinition_ID} IS NULL)
AND (cb.FTA_FarmerCredit_ID=$P{FTA_FarmerCredit_ID} OR $P{FTA_FarmerCredit_ID} IS NULL)
AND (cb.DateDoc >= $P{DateDoc1} OR COALESCE($P{DateDoc1}, Current_Timestamp) = Current_Timestamp)
AND (cb.DateDoc <= $P{DateDoc2} OR COALESCE($P{DateDoc2}, Current_Timestamp) = Current_Timestamp)
GROUP BY cl.Name, cp.TaxID, cp.Name, fc.DocumentNo, cd.DocumentNo, lot.Name,
cb.LineDescription, rl.PrintName, rl.SeqNo, cb.FTA_CreditDefinition_ID, cb.FTA_CreditDefinitionLine_ID,
cb.C_BPartner_ID, cb.FTA_FarmerCredit_ID, cb.FTA_CDL_Category_ID, cb.SO_CreditLimit, cb.SO_CreditUsed, cb.Balance,  rg.FTA_CDL_ReportGroup_ID, rg.PrintName
ORDER BY cb.FTA_CreditDefinition_ID, cb.C_BPartner_ID, cb.FTA_FarmerCredit_ID, rl.SeqNo]]>
	</queryString>
